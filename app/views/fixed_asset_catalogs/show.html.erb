 <%- model_class = FixedAssetCatalog -%>
<div class="page-header">
  <h1><%=t '.title', :default => [:'helpers.titles.new', 'New %{model}'], :model => model_class.model_name.human.titleize %></h1>
</div>
<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:code) %>:</strong></dt>
  <dd><%= @fixed_asset_catalog.code %></dd>
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @fixed_asset_catalog.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:measurement_unit) %>:</strong></dt>
  <dd><%= @fixed_asset_catalog.measurement_unit %></dd>
  <dt><strong><%= model_class.human_attribute_name(:years) %>:</strong></dt>
  <dd><%= @fixed_asset_catalog.years %></dd>
  <dt><strong><%= model_class.human_attribute_name(:desc) %>:</strong></dt>
  <dd><%= @fixed_asset_catalog.desc %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              fixed_asset_catalog_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_fixed_asset_catalog_path(@fixed_asset_catalog), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              fixed_asset_catalog_path(@fixed_asset_catalog),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => '确定删除?')) },
              :class => 'btn btn-danger' if can? :destroy, @fixed_asset_catalog%>
