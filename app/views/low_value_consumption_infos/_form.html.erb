<%= form_for @low_value_consumption_info, :html => { :class => 'form-horizontal' } do |f| %>
<%= f.error_messages %>
  <div class="control-group">
    <%= f.label :branch, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :branch, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :location, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :location, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :user, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :user, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :relevant_unit_id, :class => 'control-label' %>
    <div class="controls">
      <%= relevant_department_select_autocom("low_value_consumption_info",@low_value_consumption_info.id) %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :use_unit_id, :class => 'control-label' %>
    <div class="controls">
      <%= use_unit_select_autocom("low_value_consumption_info",@low_value_consumption_info.id) %>
    </div>
  </div>

  <%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
            low_value_consumption_infos_path(@low_value_consumption_info), :class => 'btn btn-default' %>
 
<% end %>